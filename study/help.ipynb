    "import torch\n",
    "import torchvision\n",
    "n1 = torch.nn.BatchNorm2d(1).eval()\n",
    "n2 = torchvision.ops.misc.FrozenBatchNorm2d(1).eval()\n",
    "n2.load_state_dict(n1.state_dict(), strict=False)\n",
    "x = torch.rand(1,1,1,1)\n",
    "torch.equal(n1(x), n2(x))"
   ]
  }
